#include<stdio.h>
int main(){
    int a[]={1,2,3,4,5,6,7};
    int m=sizeof(a)/sizeof(a[0]);
    int k,i;
    printf("enter the value of k :\n");
    scanf("%d",&k);
    k=k%m;
    int temp[m];
    for(i=0;i<m;i++){
        temp[(i+k)%m]=a[i];
    }
    for(i=0;i<m;i++){
        a[i]=temp[i];
    }
    for(i=0;i<m;i++){
        printf("%d ",a[i]);
    }
    return 0;
}
